<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.antares.kawalpemilu"
    android:versionCode="3"
    android:versionName="1.2" >

    <uses-permission android:name="android.permission.INTERNET" />

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="19" />

    <application
        android:name="com.antares.kawalpemilu.PemiluApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.antares.kawalpemilu.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="sensorPortrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.antares.kawalpemilu.tps.ViewTPSActivity"
            android:label="@string/app_name"
            android:screenOrientation="sensorPortrait" >
        </activity>

<!--         <receiver android:name="com.antares.kawalpemilu.client.AlarmReceiver" > -->
<!--         </receiver> -->
    </application>

</manifest>